{
  "name": "rkta-ui",
  "version": "0.0.48",
  "description": "A minimal material UI for React",
  "main": "lib",
  "scripts": {
    "dev": "docz dev",
    "docz:build": "docz build",
    "prelib": "npm run test && npm run lint && npm run docz:build",
    "lib": "babel src -d lib --copy-files",
    "lib:watch": "babel src -w -d lib --copy-files",
    "lint": "eslint src --ext .js,.jsx",
    "prettify": "prettier --write src/**/*.js*",
    "prepublish": "npm run lib",
    "shrinkwrap": "npm shrinkwrap",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.5",
    "docz": "latest",
    "docz-theme-default": "latest",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "*",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "emotion-theming": "^10.0.9",
    "lodash": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-use": "^9.4.1",
    "react-useinterval": "^1.0.1"
  },
  "author": "Serge Droganov <serge@droganov.ru>",
  "homepage": "https://github.com/r-k-t-a/rkta-ui",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/r-k-t-a/rkta-ui.git"
  },
  "bugs": {
    "url": "https://github.com/r-k-t-a/rkta-ui/issues"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
